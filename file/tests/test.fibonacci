var
  inicio
    inteiro i;
  fim

programa
  inicio
    
    enquanto(i < 16) faca
      inicio
        var inteiro val;
        val = fib(i);
        escreva(val);
        i=i+1;
      fim
  fim

funcao inteiro fib(inteiro n)
  inicio
    var inteiro fib;
    fib = 0;
    
    se(n <= 2) entao
      inicio
        fib = 1;
      fim
    senao
      inicio
        fib = fib(n-1) + fib(n-2);
      fim
  fim

  